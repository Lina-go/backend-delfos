# Azure AI Foundry
AZURE_AI_PROJECT_ENDPOINT=

# Per-agent model configurations
TRIAGE_AGENT_MODEL=gpt-4.1
INTENT_AGENT_MODEL=gpt-4o-mini
SQL_AGENT_MODEL=claude-sonnet-4-5
SQL_EXECUTOR_AGENT_MODEL=gpt-4o-mini
VERIFICATION_AGENT_MODEL=gpt-4o-mini
VIZ_AGENT_MODEL=gpt-4o-mini
FORMAT_AGENT_MODEL=gpt-4o-mini

# MCP Tools
USE_DIRECT_DB=true

# Anthropic
USE_ANTHROPIC_API_FOR_CLAUDE=false
ANTHROPIC_API_KEY=
ANTHROPIC_FOUNDRY_API_KEY=
ANTHROPIC_FOUNDRY_RESOURCE=

# MCP Server
MCP_SERVER_URL=
MCP_TIMEOUT=60
MCP_SSE_TIMEOUT=45

# Logging
LOG_LEVEL=INFO
DEBUG=false

# PowerBi
POWERBI_WORKSPACE_ID=
POWERBI_REPORT_ID=

# Warehouse (READ - agent data queries via *.datawarehouse.fabric.microsoft.com)
WH_SERVER=
WH_DATABASE=
WH_SCHEMA=gold

# SQL Database (WRITE - app CRUD via *.database.fabric.microsoft.com)
DB_SERVER=
DB_DATABASE=
DB_SCHEMA=dbo
